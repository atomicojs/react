{
  "name": "components",
  "version": "0.0.0",
  "scripts": {
    "build:types": "tsc",
    "build:exports": "exports \"dist/**/*\" --wrappers"
  },
  "dependencies": {
    "@atomico/react": "*"
  },
  "peerDependencies": {
    "@atomico/react": "*",
    "@atomico/vue": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  },
  "types": "dist/index.d.ts",
  "exports": {
    "./checkbox-list": {
      "default": "./dist/checkbox-list.js",
      "types": "./dist/checkbox-list.d.ts"
    },
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "default": "./wrapper/react.js",
      "types": "./wrapper/react.d.ts"
    },
    "./preact": {
      "default": "./wrapper/preact.js",
      "types": "./wrapper/preact.d.ts"
    },
    "./vue": {
      "default": "./wrapper/vue.js",
      "types": "./wrapper/vue.d.ts"
    },
    "./counter": {
      "default": "./dist/counter.js",
      "types": "./dist/counter.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "checkbox-list": [
        "dist/checkbox-list.d.ts"
      ],
      "react": [
        "wrapper/react.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ],
      "counter": [
        "dist/counter.d.ts"
      ]
    }
  }
}